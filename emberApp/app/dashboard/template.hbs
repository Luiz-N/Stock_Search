{{outlet}}

<div class="container-fluid">
  <h3 class="text-center">{{model.chartTitle}}</h3>

  <div class="col-md-10 col-md-offset-1">
    {{dynamic-high-charts
      chartData=model.quotes
      chartOptions=model.candleStickOptions
    }}
  </div>

  <div class="col-md-6 col-md-offset-3">
    {{symbol-search
      updateModel='updateModel'
    }}
  </div>

</div>
